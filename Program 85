#include <stdio.h>

int isVowel(char ch) {
    ch = (ch >= 'A' && ch <= 'Z') ? ch + 32 : ch;
    return ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u';
}

int main() {
    char ch;
    printf("Enter a character: ");
    scanf(" %c", &ch);
    
    if (isVowel(ch))
        printf("%c is a Vowel.\n", ch);
    else
        printf("%c is not a Vowel.\n", ch);
    
    return 0;
}
