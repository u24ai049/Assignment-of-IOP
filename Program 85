#include <stdio.h>

int Vowel(char ch)
{
    ch = (ch >= 'A' && ch <= 'Z') ? ch + 32 : ch;

    return ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u';
}

int main() 
{
    char ch;
    printf("Enter a character: ");
    scanf(" %c", &ch);
    
    if (Vowel(ch))
        printf("%c is a vowel", ch);
    else
        printf("%c is not a vwel", ch);
    
    return 0;
}
