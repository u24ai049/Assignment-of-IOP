#include <stdio.h>
#include <string.h>

int main()                     
{
    char str[100];
    printf("Enter a line of text: ");
    fgets(str,100,stdin);
    int length=strlen(str);
    printf("Length of string = %d\n",length-1);// -1 is lie liya kyu ki strlen '\n ' ko bhi count kar raha hai jab enter dabate hai
    
    return 0;
}
