#include <stdio.h>

int Max(int arr[], int s)
{
    int max = arr[0];
    for (int i = 1; i < s; i++) 
{
        if (arr[i] > max)
            max = arr[i];
    }
    return max;
}

int main()
{
    int n;

    printf("Enter the number of elements of aray");
    scanf("%d", &n);

    int arr[n];
    printf("enter the elements");
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    
    printf("Maximum element = %d\n", Max(arr, n));
    
    return 0;
}
