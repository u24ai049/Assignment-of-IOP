#include <stdio.h>

int Armstrong(int n)
{
    int sum = 0, original = n, remainder;
    
    while (n != 0)
{
        remainder = n % 10;
             sum += remainder * remainder * remainder;
        n /= 10;
    }
    return original == sum;
}

int main() 
{
    int num;

    printf("Enter a number: ");
    scanf("%d", &num);
    
    if (Armstrong(num))
        printf("%d is an Armstrong number", num);
    else
        printf("%d is not an Armstrong number\n", num);
    
    return 0;
}
