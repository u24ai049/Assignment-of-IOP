#include <stdio.h>

int factorial(int n) {
    int result = 1;
    for (int i = 1; i <= n; i++)
        result *= i;
    return result;
}

int nCr(int n, int r) {
    return factorial(n) / (factorial(r) * factorial(n - r));
}

int main() {
    int n, r;
    printf("Enter values for N and R: ");
    scanf("%d %d", &n, &r);
    
    printf("NCR = %d\n", nCr(n, r));
    
    return 0;
}
