#include<stdio.h>
#include<string.h>
int main(){
    char slice[2000];
    printf("Enter the string\n");
    fgets(slice,2000,stdin);
    printf("From what index to what index you want to slice\n");
    int m,n;
    scanf("%d %d",&m,&n);
    for(int i=m;i<=n;i++)
    {
        printf("%c",slice[i]);
    }
    
}
