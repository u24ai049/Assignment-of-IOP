#include<stdio.h>
int main(){
    int n,m;
    int arr1[n],arr2[n];
    printf("Enter the size of array \n");
    scanf("%d",&n);
    int *ptr=arr1;
    printf("Enter the elements of array 1\n");
    for(int i=0;i<n;i++){
    scanf("%p",(ptr+i));
        
    }
    for(int i=0;i<n;i++){
    arr2[i]=*(ptr+i);
        
    }
     printf("The elements of array 2 is\t");
    for(int i=0;i<n;i++)
    {
        printf("%d",arr2[i]);
    }
}
