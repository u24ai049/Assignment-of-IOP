#include <stdio.h>

void Fibonacci(int n)
{
    int a = 0, b = 1, next;
    
    for (int i = 1; i <= n; ++i) 
{
        printf("%d ", a);
        next = a + b;
        a = b;
        b = next;
    }
printf("\n");
}

int main() 
{

    int n;
    printf("Enter the number of Fibonacci terms");
    scanf("%d", &n);
    
    Fibonacci(n);
    
    return 0;
}
