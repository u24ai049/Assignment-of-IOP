#include<stdio.h>
int argument(int a)
{
    if(a==1)
    printf("0");
    else{
    for(int i=2;i<=a;i++)
    {
        if(a%i==0)
        {   
            if(a==i){
            printf("1");
                
            }break;}
        else
        {
        printf("0");
        }
    }
    }
}
int main(){
    int num;
    printf("Enter the number\n");
    scanf("%d",&num);
    argument(num);
}
