#include <stdio.h>

void exchange(int *x, int *y) 
{
    int temp = *x;
    *x = *y;
    *y = temp;
}

int main()
{
    int x, y;
    printf("Enter two numbers X and Y");
    scanf("%d %d", &x, &y);
    
    printf("Before X = %d, Y = %d\n", x, y);
    exchange(&x, &y);
    printf("After   X = %d, Y = %d\n", x, y);
    
    return 0;
}
